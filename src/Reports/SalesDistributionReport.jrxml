	<queryString language="SQL">
		<![CDATA[SELECT 
    ii.issue_date, 
    ii.Quantity AS volume, 
    ii.total_price, 
    i.Iname AS item_name, 
    c.name AS customer_name
FROM 
    issueitem ii
JOIN 
    items i ON ii.Item_id = i.Item_id
JOIN 
    customer c ON ii.customer_id = c.customer_id
WHERE 
    DATE(ii.issue_date) BETWEEN DATE($P{fromDate}) AND DATE($P{toDate})]]>
	</queryString>
